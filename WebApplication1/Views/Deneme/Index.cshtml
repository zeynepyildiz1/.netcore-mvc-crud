@model  IEnumerable<Student>

    @{
        Layout = null;
    }
    <form asp-controller="Student" asp-action="Index" method="post" enctype="multipart/form-data">
        <input type="file" name="file" />
        <button type="submit">Upload</button>
    </form>

    @if (Model.Count() > 0)
    {
        <table cellpadding="0" cellspacing="0" border="1">
            <tr>
                <td>Name</td>
                <td>Roll</td>
                <td>Email</td>
            </tr>
            @foreach (Student student in Model)
            {
                <tr>
                    <td>@student.Name</td>
                    <td>@student.Roll</td>
                    <td>@student.Email</td>
                </tr>
            }
        </table>
    }
